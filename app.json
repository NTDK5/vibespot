{
  "expo": {
    "name": "VibeSpot",
    "slug": "vibespot",
    "version": "1.0.0",
    "orientation": "portrait",
    "userInterfaceStyle": "light",
    "splash": {
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.vibespot.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff"
      },
      "package": "com.vibespot.app"
    },
    "updates": {
      "enabled": false,
      "fallbackToCacheTimeout": 0
    },
    "plugins": [
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow VibeSpot to use your location."
        }
      ],
      [
        "expo-image-picker",
        {
          "mediaLibraryPermission": "Allow VibeSpot to access your photos."
        }
      ],
      "expo-font"
    ]
  }
}
